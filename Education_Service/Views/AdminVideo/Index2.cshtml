@model IEnumerable<Education_Service.tblCourseVideo>

@{
    ViewBag.Title = "Index2";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<script src="~/AdminContent/assets/js/jquery-3.7.1.js"></script>
<script src="~/AdminContent/assets/js/bootstrap.bundle.js"></script>
<script src="~/AdminContent/assets/js/jquery.dataTables.min.js"></script>

<link href="~/AdminContent/assets/js/jquery.dataTables.min.css" rel="stylesheet" />

<script>
    $(document).ready(function () {
        $("table").dataTable(); 
    });



</script>

<h2>Index2</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CourseId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VideoTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VideoLink)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in (List<tblCourseVideo>)ViewBag.Allvideo)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CourseId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VideoTitle)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VideoLink)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                </td>
            </tr>
        }
    </tbody>

</table>
