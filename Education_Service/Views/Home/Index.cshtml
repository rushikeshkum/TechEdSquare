<section id="apply-aprt" class="pb-120">
    <div class="container">
        <div class="apply">
            <div class="row no-gutters">
                <div class="col-lg-6">
                    <div class="apply-cont apply-color-1">
                        <h3>Apply for Educator Role</h3>
                        <p>
                            Join Our Team as an Educator:

                            Embark on a fulfilling journey with TechEdSquare as we open doors to dynamic educators.. If you have a passion for education, a commitment to student success, and an innovative approach to teaching, we invite you to apply for the Educator role.
                        </p>
                        <a href="#" class="main-btn">Apply Now</a>
                    </div> <!-- apply cont -->
                </div>
                <div class="col-lg-6">
                    <div class="apply-cont apply-color-2">
                        <h3>Apply for Internship </h3>
                        <p>
                            Explore Your Potential with Our Internship Program:

                            Unlock your potential and gain hands-on experience through our rewarding internship program at TechEdSquare. We believe in nurturing talent and providing a platform for individuals to grow and thrive. Our internship opportunities are designed to offer real-world insights.
                        </p>
                        <a href="#" class="main-btn">Apply Now</a>
                    </div> <!-- apply cont -->
                </div>
            </div>
        </div> <!-- row -->
    </div> <!-- container -->
</section>

<!--====== APPLY PART ENDS ======-->
<!--====== COURSE PART START ======-->

<section id="course-part" class="pt-115 pb-120 gray-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="section-title pb-45">
                    <h5>Our course</h5>
                    <h2>Featured courses </h2>
                </div> <!-- section title -->
            </div>
        </div> <!-- row -->
        <div class="row course-slied mt-30">
            @foreach (var item in (List<tblClassCourse>)ViewBag.Allcourse)
            {

                <div class="col-lg-4">
                    <div class="singel-course">
                        <div class="thum">
                            <div class="image">
                                @if (item.CourseImage != null)
                                {
                                    <img src="~/UploadedCourseImg/@item.CourseImage" alt="Course" width="350px" />
                                }
                            </div>
                            <div class="price">
                                @Html.DisplayFor(modelItem => item.CourseFees)/-
                            </div>
                        </div>
                        <div class="cont">
                            <ul>
                                <li><i class="fa fa-star"></i></li>
                                <li><i class="fa fa-star"></i></li>
                                <li><i class="fa fa-star"></i></li>
                                <li><i class="fa fa-star"></i></li>
                                <li><i class="fa fa-star"></i></li>
                            </ul>
                            <span>(20 Reviws)</span>
                            <a href="@Url.Action("index", "courseusersingle", new { id = item.id })"><h4> @Html.DisplayFor(modelItem => item.CourseName)</h4></a>
                            <div class="course-teacher">

                                <div class="name">
                                    <a><h6>  @Html.DisplayFor(modelItem => item.tblEducatorDetail.EducatorName)</h6></a>
                                </div>
                                <div class="admin">
                                    <ul>
                                        <li><a href="#"><i class="fa fa-user"></i><span>31</span></a></li>
                                        <li><a href="#"><i class="fa fa-heart"></i><span>10</span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div> <!-- singel course -->
                    
                </div>
            }
            
        </div> <!-- course slied -->
    </div> <!-- container -->
</section>

<!--====== COURSE PART ENDS ======-->
